<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.qq12cvhj.chihuo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/chihuo"
        android:label="@string/app_name"
        android:resizeableActivity="false"
        android:roundIcon="@drawable/chihuo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:screenOrientation="portrait"
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchFoodActivity" android:screenOrientation="portrait"/>
        <activity android:name=".FoodDetailActivity"  android:screenOrientation="portrait"/>
        <activity android:name=".RegActivity" android:screenOrientation="portrait"/>
        <activity android:name=".myInfoActivity" android:screenOrientation="portrait"/>
        <activity android:name=".addNewFoodActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AddNewFoodTypeActivity" android:screenOrientation="portrait"/>
        <activity android:name=".NewShareActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>